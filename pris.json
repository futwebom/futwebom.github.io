{
  "doctype": "!DOCTYPE html",
  "html": {
    "lang": "en",
    "head": {
      "meta_charset": "UTF-8",
      "meta_viewport": {
        "name": "viewport",
        "content": "width=device-width, initial-scale=1.0"
      },
      "title": "Extrair Documento do Iframe",
      "style": {
        "#iframe-container": {
          "width": "100%",
          "height": "500px",
          "overflow": "auto"
        }
      }
    },
    "body": {
      "div": {
        "id": "iframe-container",
        "iframe": {
          "id": "myIframe",
          "src": "https://reidoscanais.app/embed/player.php?l=aHR0cHM6Ly94bi0tLS1pdGJhdmJ2YWNlaXoueG4tLS0tLS0tNDNkYmFiYmNxdnNhYzhhZmJmZHdjb2Vja3ZrbmdkMWZybGd1aGFqOWIubmV0L3d0Yy5waHA/aWQ9MTU3Nw=="
        }
      },
      "script": {
        "window.onload": "function() {\n  var iframe = document.getElementById('myIframe');\n  var iframeDocument = iframe.contentDocument || iframe.contentWindow.document;\n  var iframeContent = iframeDocument.documentElement.innerHTML;\n\n  document.getElementById('iframe-container').innerHTML = iframeContent;\n};"
      }
    }
  }
}
